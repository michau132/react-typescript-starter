{
  "name": "react-typescript-starter",
  "version": "1.0.0",
  "description": "A sample setup using React, Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack --colors --config webpack.prod.js --env.ENVIRONMENT=production",
    "start": "webpack-dev-server --config webpack.dev.js --colors --env.ENVIRONMENT=development",
    "lint": "eslint",
    "lint:fix": "eslint './src/**/*.{ts,tsx}'"
  },
  "author": {
    "name": "Michal Lutecki",
    "email": "michal1324@hotmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fork-ts-checker-webpack-plugin": "1.3.3",
    "html-webpack-plugin": "5.5.0",
    "typescript": "3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "^4.9.0",
    "react-router": "^5.0.1",
    "styled-components": "^4.2.1"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
